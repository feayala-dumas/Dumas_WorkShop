{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-sbx-fa-ju9ncr"
		},
		"LS_BlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BlobStorage'"
		},
		"LS_KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-sbx-fa-ju9ncr.vault.azure.net/"
		},
		"LS_SQL_Database_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-sbx-fa-ju9ncr.database.windows.net"
		},
		"LS_SQL_Database_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb-sbx-fa-01"
		},
		"LS_SQL_Database_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-sbx-fa-ju9ncr_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_BlobStorage_connectionString": {
						"value": "[parameters('LS_BlobStorage_connectionString')]"
					},
					"LS_KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_SQL_Database_properties_typeProperties_server": {
						"value": "[parameters('LS_SQL_Database_properties_typeProperties_server')]"
					},
					"LS_SQL_Database_properties_typeProperties_database": {
						"value": "[parameters('LS_SQL_Database_properties_typeProperties_database')]"
					},
					"LS_SQL_Database_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQL_Database_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}